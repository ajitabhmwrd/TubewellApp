<?xml version="1.0"?>
<configuration>
  <connectionStrings>
    <add name="ConnectionStringTubewellApp" connectionString="Data Source=(local);Initial Catalog=TubewellAppl;integrated Security=True; user id=sa; password=sa123" providerName="System.Data.SqlClient"/>
  </connectionStrings>
  <system.web>
    <compilation debug="true" targetFramework="4.0" />
    <authentication mode="Forms">
      <forms name=".ASPXAUTH" loginUrl="Login.aspx" defaultUrl="Login.aspx"
             protection="All" path="/" timeout="30">
      </forms>
    </authentication>

    <roleManager enabled="false"></roleManager>
  </system.web>

  <system.webServer>
    <security>
      <requestFiltering>
        <requestLimits maxAllowedContentLength="104857600"/>
      </requestFiltering>
    </security>
    <validation validateIntegratedModeConfiguration="false"/>
    <handlers>
      <add name="CaptchaImage" verb="GET" path="CaptchaImage.axd" type="MSCaptcha.CaptchaImageHandler, MSCaptcha"/>
    </handlers>
  </system.webServer>
</configuration>